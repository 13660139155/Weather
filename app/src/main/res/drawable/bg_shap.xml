<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">

    <!--表示四个角的圆角程度-->
    <corners android:radius="30dp"/>
    <!--表示背景的填充-->
    <solid android:color="#edebeb"/>
    <!--表示描边的宽度和描边的颜色-->
    <stroke
        android:width="1dp"
        android:color="#F5E8E8E8"/>

</shape>